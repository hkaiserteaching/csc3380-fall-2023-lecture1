{
  "name": "Alpine",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "hostRequirements": {
    "cpus": 2
  },
  "customizations": {
    "codespaces": {
      "openFiles": [
        "code/hello_world.cpp",
        "code/count_unique.cpp"
      ]
    },
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-vscode.cpptools-extension-pack",
        "xaver.clang-format"
      ]
    }
  },
  "remoteUser": "vscode"
}
